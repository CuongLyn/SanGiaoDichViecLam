@model List<BTL.Models.UngTuyen>

<h2>Các công việc đã ứng tuyển</h2>

<table class="table">
    <thead>
        <tr>
            <th>Tên công việc</th>
            <th>Ngày ứng tuyển</th>
            <th>Trạng thái</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CongViec?.TieuDe</td>
                <td>@item.NgayUngTuyen.ToString("dd/MM/yyyy")</td>
                <td>@item.TrangThai</td>
            </tr>
        }
    </tbody>
</table>
